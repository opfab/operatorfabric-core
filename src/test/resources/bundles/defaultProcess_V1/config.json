{
  "id": "defaultProcess",
  "name": "process.label",
  "version": "1",
  "uiVisibility": {
    "monitoring": true,
    "logging": true,
    "calendar": true
  },
  "states": {
    "messageState": {
      "name":  "message.title",
      "description": "messageState.description",
      "color": "#CC9900",
      "userCard" : {
        "template" : "usercard_message",
        "lttdVisible" : false
      },
      "templateName": "template",
      "styles": [
        "style"
      ],
      "acknowledgmentAllowed": "Always",
      "type" : "INPROGRESS"
    },
    "chartState": {
      "name": "chartDetail.title",
      "description": "chartState.description",
      "color": "#DA00DA",
      "templateName": "chart",
      "styles": [
        "style"
      ],
      "acknowledgmentAllowed": "Always",
      "type" : "FINISHED"
    },
    "chartLineState": {
      "name": "chartLine.title",
      "description": "chartLineState.description",
      "color": "#6666FF",
      "templateName": "chart-line",
      "styles": [
        "style"
      ],
      "acknowledgmentAllowed": "Always",
      "type" : "CANCELED"
    },
    "processState": {
      "name": "process.label",
      "description": "processState.description",
      "color": "#0070C0",
      "userCard" : {
        "template" : "usercard_process",
        "lttdVisible" : false
      },
      "templateName": "process",
      "styles": [
        "style"
      ],
      "acknowledgmentAllowed": "Always",
      "type" : "INPROGRESS"
    },
    "contingenciesState": {
      "name": "contingencies.title",
      "description": "contingenciesState.description",
      "templateName": "contingencies",
      "styles": [
        "contingencies"
      ],
      "acknowledgmentAllowed": "Always",
      "type" : "INPROGRESS"
    },
    "questionState": {
      "name": "questionState.title",
      "description": "questionState.description",
      "response": {
        "lock": true,
        "state": "responseState",
        "btnColor": "GREEN",
        "btnText": {
          "key": "question.button.text"
        }
      },
      "templateName": "question",
      "styles": [
        "style"
      ],
      "acknowledgmentAllowed": "OnlyWhenResponseDisabledForUser",
      "type" : "INPROGRESS"
    },
    "multipleOptionsResponseState": {
      "name": "multipleOptionsResponseState.title",
      "description": "multipleOptionsResponseState.description",
      "color": "#0070C0",
      "acknowledgmentAllowed": "Always",
      "templateName": "multipleOptionsResponseState"
    },
    "responseState": {
      "name": "response.title",
      "description": "responseState.description",
      "color": "#0070C0",
      "templateName": "response",
      "styles": [
        "style"
      ],
      "acknowledgmentAllowed": "Always",
      "type" : "INPROGRESS"
    }

